#!/bin/bash
#script de fastqc para los datos
CAMINO_1=/media/tefa/Limonada/Data_lbmgg
#DATA_1= #VARIABLE A USAR PARA LA CARPETA DE DATOS que uso como input
OUTPUT_Base=/media/tefa/Limonada/S_Urrea
mkdir -p $OUTPUT_Base/CALIDAD_fastqc #HACIENDO UN DIRECTORIO PADRE PARA LOS DATOS DEL CONTROL DE CALIDAD
SALIDA_1=$OUTPUT_Base/CALIDAD_fastqc
fastqc -o $SALIDA_1 --noextract -f fastq $CAMINO_1/NGS14-B660_smarter2_nextera_CGTACTAG_L006_R1_001.fastq.gz \
$CAMINO_1/NGS14-B660_smarter2_nextera_CGTACTAG_L006_R2_001.fastq.gz \
$CAMINO_1/NGS14-B705_P_Q_AGGCAGAA_L004_R1_001.fastq.gz \
$CAMINO_1/NGS14-B705_P_Q_AGGCAGAA_L004_R2_001.fastq.gz \
$CAMINO_1/NGS14-B706_R_S_GGACTCCT_L004_R1_001.fastq.gz \
$CAMINO_1/NGS14-B706_R_S_GGACTCCT_L004_R2_001.fastq.gz \
$CAMINO_1/NGS14-B707_T_TAGGCATG_L004_R1_001.fastq.gz \
$CAMINO_1/NGS14-B707_T_TAGGCATG_L004_R2_001.fastq.gz \
$CAMINO_1/NGS14-B807_W_GGACTCCT_L007_R1_001.fastq.gz \
$CAMINO_1/NGS14-B807_W_GGACTCCT_L007_R2_001.fastq.gz \
$CAMINO_1/NGS14-B808_G_TCCTGAGC_L008_R1_001.fastq.gz \
$CAMINO_1/NGS14-B808_G_TCCTGAGC_L008_R2_001.fastq.gz
